  #include <iostream>
using namespace std;

class csbs
{
	private:
		char a;
		public:
		void vsdh()
		{
			cout<<"enter a vowel";
			cin>>a;
switch(a)
{
	
	case 'a':
	case 'A':
	case 'e':
	case 'E':
	case 'i':
	case 'I':
	case 'o':
	case 'O':
	case 'u':
	case 'U':
		cout<<"\n\t\t\t\tIT IS A VOWEL";
	break;
	default:
	cout<<"\n\t\t\tIT IS NOT A VOWELl";
	}}};
int main()
{
csbs vs;
vs.vsdh();
